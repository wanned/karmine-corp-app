{
  "expo": {
    "name": "Karmine Plus",
    "slug": "karmine-plus",
    "version": "0.1.0",
    "experiments": {
      "tsconfigPaths": true
    },
    "orientation": "portrait",
    "icon": "./assets/icons/icon.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/icons/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#101E35"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/icons/adaptive-icon.png",
        "backgroundColor": "#101E35",
        "monochromeImage": "./assets/icons/adaptive-icon.png"
      },
      "package": "plus.karmine.karmineplus"
    },
    "plugins": [
      "expo-localization",
      "expo-font"
    ],
    "ios": {
      "bundleIdentifier": "plus.karmine.karmineplus"
    }
  }
}
