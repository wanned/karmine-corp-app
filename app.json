{
  "expo": {
    "name": "Karmine Plus",
    "slug": "karmine-plus",
    "version": "0.2.0",
    "experiments": {
      "tsconfigPaths": true
    },
    "orientation": "portrait",
    "icon": "./assets/icons/icon.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/icons/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#101E35"
    },
    "notification": {
      "icon": "./assets/icons/notification.png"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/icons/adaptive_icon.png",
        "backgroundColor": "#101E35",
        "monochromeImage": "./assets/icons/adaptive_icon.png"
      },
      "googleServicesFile": "./google-services.json",
      "package": "plus.karmine.karmineplus"
    },
    "ios": {
      "googleServicesFile": "./GoogleService-Info.plist",
      "bundleIdentifier": "plus.karmine.karmineplus"
    },
    "plugins": [
      "expo-localization",
      "expo-font",
      "@react-native-firebase/app",
      "@react-native-firebase/messaging",
      "expo-build-properties",
      [
        "expo-build-properties",
        {
          "ios": {
            "useFrameworks": "static"
          }
        }
      ]
    ],
    "extra": {
      "eas": {
        "projectId": "8e4a6552-6274-4d25-9f8e-73c74ad6c266"
      }
    }
  }
}
